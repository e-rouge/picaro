{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "useUnknownInCatchVariables": true,
    "baseUrl": ".",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@api": [
        "./utils/api"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@libs/*": [
        "./../Libs/*"
      ],
      "@types": [
        "./src/types"
      ],
      "@data": [
        "./../Data"
      ],
      "@plugins/modules/*": [
        "./../../plugins/modules/*"
      ],
      "@fixtures/*": [
        "./fixtures/*"
      ]
    }
  },
  "types": [
    "node"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.vue"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "vueCompilerOptions": {
    "target": 3.4
  }
}
